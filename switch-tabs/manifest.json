{
  "manifest_version": 2,

  "name": "Switching tabs",
  "description": "Tab switching hotkeys",
  "version": "1.0",

  // "browser_action": {
    // "default_icon": "icon.png",
    // "default_popup": "popup.html"
  // },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],

  "background": {
      "scripts": ["background.js"],
      "persistent":  false
    },
  "commands": {
    "switch-left": {
      "suggested_key": {
        "default": "Ctrl+Left"
      },
      "description": "Switch tab to left"
    },
    "switch-right": {
      "suggested_key": {
        "default": "Ctrl+Right"
      },
      "description": "Switch tab to right"
    }    

  }
}